-
    var rout = [
        {'name': 'Главная', 'link' : "./index.html"},
        {'name': 'Баланс и переводы', 'link' : "./"},
    ];
.container
    nav(aria-label="breadcrumb")
        ol.breadcrumb
            each val, index in rout
                li.breadcrumb-item
                    a(href=val.link, title=val.name)=val.name